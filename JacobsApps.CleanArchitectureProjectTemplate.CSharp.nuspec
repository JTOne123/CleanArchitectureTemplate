<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>JacobsApps.CleanArchitectureProjectTemplate.CSharp</id>
    <version>2.6.3</version>
    <description>
      Creates a "clean architecture" project setup for a MVC / WebApi / Blazor project.
    </description>
    <releaseNotes>Added multiple solution files to create a runnable project in GitHub. No need to replace vars in the sln files anymore, the different project types should run directly from GitHub too.</releaseNotes>
    <authors>Jacob Duijzer</authors>
    <projectUrl>https://github.com/jacobduijzer/CleanArchitectureTemplate</projectUrl>
    <license type="expression">MIT</license>
    <packageTypes>
      <packageType name="Template" />
    </packageTypes>
    <tags>csharp cleanarchitecture template project dotnetcore core netcoreapp3.1 mediator mediatr blazor</tags>
  </metadata>
  <files>
    <file src=".\src\**" target="content" exclude="appveyor.yml;*.nuspec;**\bin\**;**\obj\**;**\.vs\**;**\*.user;**\.idea\**"/>
    <file src=".\src\.template.config\template.json" target="content\.template.config\template.json"/>
  </files>
</package>
